    <div class="max-site-width md--up--px2 page-sub-section">
      <div class="lg--up--ten-twelfths mx--auto px2 md--up--px0">
        <div class="grid">
          <div class="grid__item md--up--four-twelfths lg--up--three-twelfths">
            <div class="detail__container">
              <div class="detail__title text__highlight text__highlight--2">Urs Mätzler</div>
              <div class="detail__container--with-image">
                <div class="detail__container--img">
                  <img src="https://cdn.shopify.com/s/files/1/0074/3859/8244/files/map-sankt-gallen.svg?28095" alt="Map of Sankt Gallen" class="detail__img"/>
